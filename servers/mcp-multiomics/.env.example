# Multi-Omics Server Configuration

# Data directories
MULTIOMICS_DATA_DIR=/workspace/data/multiomics
MULTIOMICS_CACHE_DIR=/workspace/cache/multiomics

# R configuration (optional - will auto-detect if not set)
# MULTIOMICS_R_HOME=/usr/lib/R

# Enable mock mode (set to false for real analysis with R)
MULTIOMICS_DRY_RUN=true

# Analysis parameters
MULTIOMICS_MAX_FEATURES=5000
MULTIOMICS_MIN_SAMPLES=3
MULTIOMICS_FDR_THRESHOLD=0.05
MULTIOMICS_STOUFFER_WEIGHTS=true

# Logging
MULTIOMICS_LOG_LEVEL=INFO

# Performance
MULTIOMICS_N_JOBS=-1
MULTIOMICS_TIMEOUT_SECONDS=600
